				"ext.fileannotations.FileAnnotator"
			]
		},
		"ext.fileannotations.FileAnnotator": {
			"scripts": [
				"resources/src/FileAnnotator.js"
			],
			"styles": [
				"resources/src/FileAnnotator.less"
			],
			"dependencies": [
				"ext.fileannotations.FileAnnotation",
